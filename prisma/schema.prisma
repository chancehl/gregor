generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Squad {
  id          Int      @id @default(autoincrement())
  name        String
  ownerId     String
  region      Region?
  summonerIds String[]

  @@map("squads")
}

enum Region {
  NA
  EU
  KR
}
